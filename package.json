{
  "name": "test-pkg-rl",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "bin": "./build/index.js",
  "scripts": {
    "clean": "pnpm clean:build && pnpm clean:dist",
    "clean:build": "shx rm -rf ./build",
    "clean:dist": "shx rm -rf ./dist",
    "dev": "ts-node ./src/index.ts",
    "build": "pnpm clean:build && tsc",
    "preview": "pnpm run build && node build/index.js",
    "pkg:mac": "pnpm clean:dist && pnpm build && pkg . && ./buildMacApp.sh",
    "pkg:win": "pnpm clean:dist && pnpm build && pkg .",
    "app": "./dist/test-pkg-rl"
  },
  "pkg": {
    "scripts": "build/*.js",
    "assets": [],
    "targets": [ "host" ],
    "outputPath": "dist"
  },
  "keywords": [],
  "author": "tde-roqu",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "pkg": "^5.8.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
